{
  "name": "trade-xyz-dashboard",
  "version": "1.0.0",
  "description": "A comprehensive trading dashboard built with React and TypeScript",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "axios": "^1.6.5",
    "zustand": "^4.4.5",
    "recharts": "^2.10.3",
    "date-fns": "^2.30.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.40",
    "@types/react-dom": "^18.2.16",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.16"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
